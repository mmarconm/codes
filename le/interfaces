

iface eth0 inet static
	address 192.168.1.254
	netmask 255.255.255.0
	gateway 192.168.1.1
	dns-nameserver 8.8.8.8 8.8.4.4 192.168.1.1
	broadcast 192.168.1.255

iface eth0 inet static
	address 192.168.0.250
	netmask 255.255.255.0
	gateway 192.168.0.1
	dns-nameserver 8.8.8.8 8.8.4.4 192.168.1.1
	broadcast 192.168.0.255
